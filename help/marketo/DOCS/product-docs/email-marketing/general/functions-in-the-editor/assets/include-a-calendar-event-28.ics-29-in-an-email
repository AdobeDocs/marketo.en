<script type="text/javascript">

AJS.$(window).load(function () {
    if (AJS.params.remoteUser == ''){
          AJS.$('body').addClass("anon");
    } else {
          AJS.$('body').addClass("user");
    }

    //send external links via new tab
    $("a.external-link").attr("target","_blank");

    // use -- to denote an image that should have no style.
    $("img[alt*='--']").addClass("noStyle");

});


function parenReplace () {
    $('.plugin_pagetree a, ul.childpages-macro a,#title-text a,#breadcrumbs a').each(function(){
           var name = $(this).text().replace( /\([^(]*\)/g, "" );
           $(this).text(name);
    });
}

var movedFAQ = false;
function moveFAQ() {

        var lastLink = $(".plugin_pagetree_children_list>li").last();
        $( "<li><a href='/questions' class='faqLink'>Frequently Asked Questions</a></li>" ).insertAfter(lastLink);
}

//Ran's attempt to open one extra layer of the pagetree each time user clicks
function openTreeNode() {
   var curr= $('.plugin_pagetree_children_content .plugin_pagetree_current a').parent().parent().prev().children();
   if (curr.hasClass("icon-section-closed")) {
      curr.click();
   }
}


jQuery( document ).ajaxComplete(
    function handleComplete( event, jXHR, config ) {
        if ( config.url.indexOf( "/plugins/pagetree/naturalchildren.action" ) !== -1 ) {

           parenReplace();
       
          // if (!movedFAQ) {
          //     moveFAQ();
          //     movedFAQ = true;
          // }
           openTreeNode();
        }
    }
); 




$(function() {
// Superball
  var menuHtml = "<div class='mktoNav' style='display:none;'>"+
          "<div class='mktoInbound'>"+
        "<a class='mktoHasIcon mktoMkto' href='https://app.marketo.com/integration/marketoappint?dl_token=%23MM' target='myMarketo'>My Marketo</a>"+
        "<a class='mktoHasIcon mktoDocs' href='/'>Docs Home</a>"+
        "<a class='mktoHasIcon mktoNation' href='http://nation.marketo.com' target='_blank'>Community</a>"+
        "<a class='mktoHasIcon mktoPtn' href='http://launchpoint.marketo.com' target='_blank'>LaunchPoint</a>"+
        "<a class='mktoHasIcon mktoDev' href='http://developers.marketo.com' target='_blank'>Developers</a>"+
      "</div>" +         
        "</div>"


        $('#logo').after(menuHtml);

        var SimpleSlideMenu = function (trigger, menu){
           var isShown = false;
            var navItem = trigger;
            var nav = menu; 
            navItem.click(function (e){
              e.preventDefault();
              if(!isShown){
                nav.slideDown();
                isShown = true;      
              }else{
                nav.slideUp();
                isShown = false;
              }
            });
            $('body').click(function (evt){
              if(!isShown) return;
              //A click anywhere except inside the button or the dropdown should close the dropdown.
              if((!(navItem.is(evt.target) || navItem.find(evt.target).length ||
                  nav.is(evt.target) || nav.find(evt.target).length))){
                nav.slideUp();
                isShown = false;
              }
            });             
        }
        SimpleSlideMenu($("#logo a"), $('.mktoNav'));
 var spaceName =( AJS.params.spaceKey === "JPN") ? "日本語" : "English";
// var spaceLink = "https://docs.marketo.com/display/"+ spaceKey; 
// var lang = '<li id="lang-select"><a class="' + spaceKey + '" href="'+ spaceLink+ '"></a></li>';
  var lang = '<li id="lang-select"><a href="#">'+ spaceName + '</a></li>'
             + '<ul id="lang-dropdown"><li><a href="https://docs.marketo.com/display/docs">English</a></li><li><a href="https://docs.marketo.com/display/jpn">日本語</a></li></ul>';
      $('.aui-header-secondary>ul>li').first().before(lang);
      $('#lang-select').click(function(){
			$('#lang-dropdown').toggle();
		});
      });    
   
</script>

